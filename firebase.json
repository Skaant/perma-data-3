{
  "hosting": {
    "public": "public",
    "redirects": [
      {
        "source": "/",
        "destination": "/fr",
        "type" : 302
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
